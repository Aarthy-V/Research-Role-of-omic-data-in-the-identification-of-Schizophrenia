    "#Label Encode the last column which is target \n",
    "\n",
    "from sklearn.preprocessing import LabelEncoder\n",
    "\n",
    "last_column = df_transposed.iloc[:, -1]  # Extract the last column\n",
    "encoder = LabelEncoder()\n",
    "encoded_last_column = encoder.fit_transform(last_column)\n",
    "\n",
    "# Add the encoded column back to the DataFrame\n",
    "df_transposed['Target'] = encoded_last_column\n",
    "df_transposed.head()\n"
   ]
