   "metadata": {},
   "source": [
    "# Feature selection using Mutual Information"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "4c94b3b1",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Define X and y\n",
    "\n",
    "from sklearn.feature_selection import mutual_info_classif\n",
    "\n",
    "X = df_transposed.drop(columns=[\"Target\"])\n",
    "y = df_transposed[\"Target\"]\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "cdb78b9c",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "3919     0.155551\n",
      "57660    0.152460\n",
      "51917    0.152150\n",
      "11655    0.151376\n",
      "43194    0.149740\n",
      "           ...   \n",
      "19217    0.000000\n",
      "19218    0.000000\n",
      "39317    0.000000\n",
      "39316    0.000000\n",
      "0        0.000000\n",
      "Name: MI Scores, Length: 60676, dtype: float64\n"
     ]
    }
   ],
   "source": [
    "# Calculate mutual information scores using scikit-learn's mutual_info_classif\n",
    "mi_scores = mutual_info_classif(X, y)\n",
    "\n",
    "# Create a pandas Series with feature names as indices\n",
    "mi_scores_series = pd.Series(mi_scores, index=X.columns, name=\"MI Scores\")\n",
    "\n",
    "# Sort the Series in descending order\n",
    "mi_scores_series = mi_scores_series.sort_values(ascending=False)\n",
    "\n",
    "# Print the mutual information scores\n",
    "print(mi_scores_series)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "cd27b347",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Create a bar plot for mutual information scores to view\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "plt.figure(figsize=(10, 6))\n",
    "mi_scores_series.plot(kind=\"bar\")\n",
    "plt.title(\"Mutual Information Scores for Dataset Features\")\n",
    "plt.xlabel(\"Features\")\n",
    "plt.ylabel(\"MI Score\")\n",
    "plt.xticks(rotation=45)\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
